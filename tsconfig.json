{
  "compilerOptions": {
  "target": "es2016",
  "experimentalDecorators": true,
  "emitDecoratorMetadata": true,
  "module": "commonjs",
  "baseUrl": "./src",
  "paths": {
    "@databases/*":["databases/*"],
    "@modules/*":["modules/*"],
    "@config/*":["config/*"],
    "@shared/*":["shared/*"],
    "@errors/*":["errors/*"],
    "@utils/*":["utils/*"],
    "@routes/*":["routes/*"],
    "@middlewares/*":["middlewares/*"]
  },
  "resolveJsonModule": true,         
  "outDir": "./dist",
  "esModuleInterop": true,
  "forceConsistentCasingInFileNames": true,
  "skipLibCheck": true
  }
}
